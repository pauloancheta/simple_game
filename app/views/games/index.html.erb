<div class="board">
 <% x = 0 %>

 <table>
   <tr>
     <% while x < 10 %>
       <td>
         <% y = rand(2) %>
         <% if y > 0 %>
           <div class="enemy" id="<%= x %>"></div>
         <% end %>
       </td>
       <% x += 1 %>
     <% end %>
   </tr>
 </table>


  <div class="spaceship">
    <center></center>
  </div>
</div>
